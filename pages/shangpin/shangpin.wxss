page {
  width: 100%;
  height: 100%;
   background-color:rgb(242, 242, 242);
}
.index-page{
  width: 100%;
  height: 100%;
}
.pic{
width: 100%;
height: 300rpx;
}
.pic image{
  width: 100%;
  height: 100%;
}
/* 商品信息栏 */
.spinfo{
width: 100%;
height: 350rpx;
background: white;
display: flex;
flex-direction: column;
}
.title{
  width: 100%;
  display: flex;
  align-items: center;
  height: 25%;
  margin-top: 35rpx;
}
.zp{
  width: 10%;
  height: 58%;
   background: linear-gradient(to right, yellow , orange);
   border-top-left-radius: 4px;
   border-bottom-right-radius: 4px;
   margin-left: 25rpx;
   font-size: 15rpx;
   display: flex;
   justify-content: center;
   align-items: center;
}
.sp-title{
  margin-left: 18rpx;
  width: 70%;
  height: 100%;
  font-size: 42rpx;
  font-weight: 700;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.font{
  text-align: center;
}
.info{
   margin-left: 25rpx;
  width: 100%;
  height: 30%;
  margin-top: 20rpx;
  font-size: 26rpx;
  color: rgba(0, 0, 0, 0.5);
  display: flex;
}
.peisong{
  margin-left: 25rpx;
}
.last{
  width: 100%;
  height: 25%;
  display: flex;
    margin-left: 15rpx;
    align-items: center;
    justify-content: space-between;
}
.money{
  width: 30%;
  height: 100%;
  font-size: 38rpx;
  color: red;
  display: flex;
  align-items: center;
}
.fuhao{
  font-size: 32rpx
}
.btn{
  width: 25%;
  height: 80%;
  background: rgb(254 ,204, 107);
    font-size: 26rpx;
    border-radius: 4px;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-right: 30rpx;

}
/* 途径 */
.tujin{
  margin-top: 25rpx;
  width: 100%;
  height: 120rpx;
  background: white;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 26rpx;
}
.type{
  margin-left: 25rpx;
    color: rgba(0, 0, 0, 0.5);
}
.leixing{
  text-align: center;
