#address{
  width: 100%;
  padding-bottom: 80rpx;
}
.addressList{
  margin-left: 5%;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  position: relative;
  border-bottom: 1rpx solid #E5E5E5;

}
.address-pos{
  font-size: 28rpx;
  width: 80%;
  display: flex;
}
.address-pos-isDefault{
  font-size: 20rpx;
  height: 30rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
  line-height: 30rpx;
  background:#fff;
  text-align: center;
  color: #fff;
  margin-right: 20rpx;
  margin-top: 8rpx;
}
.address-bottom{
  font-size: 23rpx;
  color: #A9A9A9;
  display: flex;
}
.address-name{
  margin-right: 20rpx;
}
.icon-edit{
  position: absolute;
  right: 5%;
  top: 50%;
  font-size: 38rpx;
  width: 50rpx;
  height: 50rpx;
  margin-top: -30rpx;
  line-height: 50rpx;
}
.add-btn{
  position: fixed;
  width: 100%;
  height: 80rpx;
  background: green ;
  bottom: 0;
  text-align: center;
  line-height: 80rpx;
  color: #fff;
  font-size: 25rpx;
  display: flex;
  justify-content: center;
}
.add-btn-text{
  margin-left: 10rpx;
}