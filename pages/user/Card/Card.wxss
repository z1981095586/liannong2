.tabBar {
  width: 90%;
  padding-left: 5%;
  padding-right: 5%;
  height: 80rpx;
  background: #fff;
  border-top: 2rpx solid #f5f5f5;
  border-bottom: 1rpx solid #f5f5f5;
  display: flex;
}
.tab-item {
  display: inline-block;
  flex: 1;
  text-align: center;
  line-height: 80rpx;
  font-size: 28rpx;
}

.active {
  border-bottom: 3rpx solid #ff3434;
  color: #ff3434;
}
