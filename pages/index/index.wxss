  border-radius: 10rpx;
}

.conter {
  width: 100%;
  height: 100%;
  animation: opaque 12s forwards;
}
.conter-no {
  width: 100%;
  height: 100%;
    animation: opaque-no 2s forwards;
}
@keyframes opaque-no{
  0%{
    opacity: .0;
  }
  100%{
    opacity: 1；
  }
}
@keyframes opaque {
  0% {
    opacity: 0.0;
  }

  60% {
    opacity: 0.0;
  }

  100% {
    opacity: 1;
  }
}
